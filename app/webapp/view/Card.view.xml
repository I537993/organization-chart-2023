<mvc:View
	controllerName="com.sap.robby.orgchart.controller.Card"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
            <QuickViewCard
                navigate=".onNavigate"
                afterNavigate=".onAfterNavigate"
                pages='{/Us}'
                >
                
                <QuickViewPage
                    pageId="{ID}"
                    header="header"
                    title="{firstname} {lastname}"
                    titleUrl=""
                    description="{company}">
                    <avatar>
                        <Avatar
                        />
                    </avatar>
                    <QuickViewGroup elements='{members}' heading='Team Leads'>
                        <QuickViewGroupElement type='pageLink' pageLinkId='{ID}' label='{company}' value='{firstname} {lastname}'/>
                    </QuickViewGroup>
                </QuickViewPage>
            </QuickViewCard>
</mvc:View>