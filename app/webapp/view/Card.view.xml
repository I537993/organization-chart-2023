<mvc:View
	controllerName="com.sap.robby.orgchart.controller.Card"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
            <QuickViewCard
                navigate=".onNavigate"
                afterNavigate=".onAfterNavigate">
                <QuickViewPage
                    pageId="{ID}"
                    header="header"
                    title="{firstname} {lastname}"
                    titleUrl=""
                    description="CoE NA Cloud Technology">
                    <avatar>
                        <Avatar
                        />
                    </avatar>
                </QuickViewPage>
                <QuickViewGroup
                heading="Teams"
                elements="{
                    path: '{ID}/managing',
                    templateShareable: false
                }"
            </QuickViewCard>
</mvc:View>